<header>

    
        {{#each appposes}}
            
            <div class = "wordPoses">
                <h3><center>{{name}}</center></h3>
                <h2 style= "color: #46D9E3;"><center>{{sanskrit}}</center></h2>
            </div>
            <div class = "mediaPoses">
                <center><img width= "200" height= "200" src= "{{imgURL}}"></center>
                <center><iframe width="300" height="315" src="{{videoURL}}"></iframe></center>
            </div>

        {{/each}}

        <script src="https://code.jquery.com/jquery.js"></script>
        <script src="js/bootstrap.min.js"></script>
        <script src="js/menu.js"></script>
        <script src="js/appposes.js"></script>
 
